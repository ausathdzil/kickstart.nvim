{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "?.lua",
    "?/init.lua"
  ],
  "diagnostics.globals": ["vim"],
  "diagnostics.disable": ["missing-fields", "undefined-doc-name"],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME",
    "${3rd}/luv/library"
  ]
}
